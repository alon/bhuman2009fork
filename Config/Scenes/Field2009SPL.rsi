  <Macro name="graycarpet">
    <Compound>
      <Elements>
        <ComplexShape name="graycarpet">
          <Material name="Carpet"/>
          <Appearance ref="field-grey"/>
          <GraphicalRepresentation vertexList="carpet coords">
            <Quad v1="greyCarpetRightXCorner"
		  v2="greyCarpetLeftXCorner"
		  v3="greyCarpetLeftYCorner"
		  v4="greyCarpetRightYCorner"/>
            <GraphicalAttributes>
              <SmoothSurface value="false"/>
              <CombinePrimitives value="true"/>
            </GraphicalAttributes>
          </GraphicalRepresentation>
          <PhysicalRepresentation>
            <SimpleBox name="back" length="8.4" width="7.4" height="0.5">
              <Translation x="0.0" y="0.0" z="-0.25"/>
              <PhysicalAttributes>
                <Mass value="10"/>
              </PhysicalAttributes>
            </SimpleBox>
            <SimpleBox name="side1" length="8.4" width="0.5" height="0.5">
              <Translation y="-3.45" z="0.25"/>
              <PhysicalAttributes>
                <Mass value="10"/>
              </PhysicalAttributes>
            </SimpleBox>
            <SimpleBox name="side2" length="8.4" width="0.5" height="0.5">
              <Translation y="3.45" z="0.25"/>
              <PhysicalAttributes>
                <Mass value="10"/>
              </PhysicalAttributes>
            </SimpleBox>
            <SimpleBox name="side3" length="0.5" width="7.4" height="0.5">
              <Translation x="4.45" z="0.25"/>
              <PhysicalAttributes>
                <Mass value="10"/>
              </PhysicalAttributes>
            </SimpleBox>
            <SimpleBox name="side4" length="0.5" width="7.4" height="0.5">
              <Translation x="-4.45" z="0.25"/>
              <PhysicalAttributes>
                <Mass value="10"/>
              </PhysicalAttributes>
            </SimpleBox>
          </PhysicalRepresentation>
        </ComplexShape>
      </Elements>
    </Compound>
  </Macro>

