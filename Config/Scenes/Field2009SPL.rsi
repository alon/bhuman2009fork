  <Macro name="field">
    <Compound>
      <Elements>        


	<Use macroName="whiteLineAroundFieldLong" instanceName="whiteLine">
          <Translation x="0.0" y="2.7125" z=".0"/>
	</Use>
	<Use macroName="whiteLineAroundFieldLong" instanceName="whiteLine">
          <Translation x="0.0" y="-2.7125" z=".0"/>
	</Use>
	<Use macroName="whiteLineAroundFieldShort" instanceName="whiteLine">
          <Translation x="3.7125" y=".0" z=".0"/>
	</Use>
	<Use macroName="whiteLineAroundFieldShort" instanceName="whiteLine">
          <Translation x="-3.7125" y=".0" z=".0"/>
	</Use>

        <Use macroName="graycarpet" instanceName="greyCarpet yellow all">
	  <Translation x="0.0" y="0.0" z="-0.001"/>
	</Use>
        
        <!--
         <Use macroName="greencarpet" instanceName="greenCarpet green all"/>
        -->
        
        <!-- Field squirrel -->
        <Use macroName="fieldWithoutStripes" instanceName="fieldWithoutStripes green"/>
        <Use macroName="fieldWithoutStripes" instanceName="fieldWithoutStripes green">
         <Rotation x="0" y="0" z="180"/>
        </Use>
        
        <!-- Field Lines and Crosses -->
        <Use macroName="midFieldLine" instanceName="midFieldLine"/>
        <Use macroName="midFieldLine" instanceName="midFieldLine">
         <Rotation x="0" y="0" z="180"/>
        </Use>
         
        <Use macroName="fieldLines" instanceName="fieldLines white"/>

        <Use macroName="fieldLines" instanceName="fieldLines white"> 
          <Rotation x="0" y="0" z="180"/>
        </Use>

        <Use macroName="cross" instanceName="cross white"/>

        <Use macroName="cross" instanceName="cross white">
          <Translation x="1.2" y="0" z="0"/>
        </Use>  

        <Use macroName="cross" instanceName="cross white">
          <Translation x="-1.2" y="0" z="0"/>
        </Use>  

        <Use macroName="quarter circle 40" instanceName="quarter circle 40"/>
        <Use macroName="quarter circle 40" instanceName="quarter circle 40">
          <Rotation x="0" y="0" z="180"/>
        </Use>

        <Use macroName="support triangles" instanceName="outer own goal">
          <Translation x="-3" y="0" z="0"/>
        </Use>
        <Use macroName="support triangles" instanceName="outer opponent goal">
          <Rotation x="0" y="0" z="180"/>
          <Translation x="3" y="0" z="0"/>
        </Use>
        <Use macroName="goal" instanceName="own goal">
          <Translation x="-3.05" y="0" z="0"/>
          <SubstituteAllSurfaces with="field-yellow"/>
        </Use>
        <Use macroName="goal" instanceName="opponent goal">
          <Rotation x="0" y="0" z="180"/>
          <Translation x="3.05" y="0" z="0"/>
          <SubstituteAllSurfaces with="field-blue"/>
        </Use>
 	<Use macroName="crowd" instanceName="crowd1">
	  <Translation x="-4.0" y="0" z="0"/>
	</Use>
	<Use macroName="crowd" instanceName="crowd2">
	  <Translation x="4.0" y="0" z="0"/>
	  <Rotation x="0" y="0" z="180"/>
	</Use>
	<Use macroName="crowd" instanceName="crowd3">
	  <Translation x="0" y="3.0" z="0"/>
	  <Rotation x="0" y="0" z="-90"/>
	</Use>
	<Use macroName="crowd" instanceName="crowd4">
	  <Translation x="0" y="-3.0" z="0"/>
	  <Rotation x="0" y="0" z="90"/>
	</Use>
        <Use macroName="net" instanceName="own net">
